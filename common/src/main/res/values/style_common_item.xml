<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CommonItemBar">

        <attr name="dividerHeight" format="dimension|reference" />
        <attr name="dividerColor" format="color|reference" />
        <attr name="dispatchTouchEventType" format="enum">
            <enum name="def" value="0" />
            <enum name="yes" value="1" />
            <enum name="no" value="2" />
        </attr>
        <attr name="interceptTouchEventType" format="enum">
            <enum name="def" value="0" />
            <enum name="yes" value="1" />
            <enum name="no" value="2" />
        </attr>
        <attr name="leftText" format="string|reference" />
        <attr name="leftTextSize" format="dimension" />
        <attr name="rightText" format="string|reference" />
        <attr name="rightHintText" format="string|reference" />
        <attr name="rightTextSize" format="dimension" />
    </declare-styleable>
    <color name="dividerColor">#ebeced</color>
    <color name="white">#FFFFFF</color>

</resources>